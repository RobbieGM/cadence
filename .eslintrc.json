{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base",
        "prettier",
        "plugin:jsx-a11y/recommended",
        "plugin:solid/typescript"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "solid",
        "jsx-a11y",
        "@typescript-eslint"
    ],
    "rules": {
        "import/extensions": 0,
        "import/no-unresolved": 0,
        "no-debugger": 1,
        "no-unused-vars": 0,
        "@typescript-eslint/no-unused-vars": 1
    }
}
